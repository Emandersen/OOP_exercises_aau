package session3.Exercise3;

public class Rectangle {
    int width;
    int height;
    int edge;

    Rectangle(){
        width = 10;
        height = 5;
        edge = 4;
    }

    public double area(){
        return width * height;
    }

    public double circumference(){
        return 2 * (width + height);
    }

    public int edges(){
        return edge;
    }
}
