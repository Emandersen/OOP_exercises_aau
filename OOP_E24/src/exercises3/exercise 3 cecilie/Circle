package session3.Exercise3;

public class Circle {
    int radius;
    int edge;
    
    Circle(){
        radius = 10;
        edge = 0;
    }

    public double area(){
        return 3.14 * (radius * radius);
    } 

    public double circumference(){
        return 2 * 3.14 * radius;
    }

    public int edges(){
        return edge;
    }
}
