package session3.Exercise2;


public class Demo {

    public static void main(String[] args) {
        Red red = new Red();
        System.out.println("Red values:");
        System.out.println("Red intensity: " + red.getRed() + " Green intensity: " + red.getGreen() 
        + " Blue intensity: " + red.getBlue());

        System.out.println("Cyan values:");
        Cyan cyan = new Cyan();
        System.out.println("Red intensity: " + cyan.getRed() + " Green intensity: " + cyan.getGreen()
        + " Blue intensity: " + cyan.getBlue());

    }
}
